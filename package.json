{
  "name": "es-utils",
  "version": "2.1.2",
  "description": "Various utilities for working in the ECMAScript/TypeScript codebase with less friction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "node --test",
    "build": "tsc",
    "wiki": "rm -f ../es-utils.wiki/*.md && typedoc --excludeInternal && cp ./wiki/*.md ../es-utils.wiki/",
    "type-check": "tsc --noEmit --strict",
    "release": "node release.mjs",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "patch-package",
    "prepare": "npm run lint && npm run type-check && npm run build && npm run wiki"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScheiBig/es-utils.git"
  },
  "author": "Marcin Jeznach",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ScheiBig/es-utils/issues"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/ScheiBig/es-utils#readme",
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.2.1",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-codegen": "^0.33.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "patch-package": "^8.0.1",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.17",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  }
}
